package blocks;

public class Nb3 {
	
	static int a =10;
	int b=20;
	{
		System.out.println("i am a non static block");
		System.out.println(a+" "+b);
		a=100;
		b=200;
	}
	
	public static void main(String[] args) {
		System.out.println("main method started");
		new Nb3();
		System.out.println(a+" "+b);
		
	}

}
